<?xml version="1.0" encoding="UTF-8" ?>


<testCase>

    <case type="query">
        <sql>select castToInteger(?);</sql>
        <assertion id="01" parameters="['0']" expected="numeric_001.xml"/>
        <assertion id="02" parameters="['1']" expected="numeric_001.xml"/>
        <assertion id="03" parameters="['-1']" expected="numeric_001.xml"/>
        <assertion id="04" parameters="['12.34']" expected="numeric_001.xml"/>
        <assertion id="05" parameters="['-12.34']" expected="numeric_001.xml"/>
        <assertion id="06" parameters="['2147483647']" expected="numeric_001.xml"/>
        <assertion id="07" parameters="['-2147483648']" expected="numeric_001.xml"/>
        <assertion id="08" parameters="['2147483648']" expected="error: 参数有误"/>
        <assertion id="09" parameters="['-2147483649']" expected="error: 参数有误"/>
        <assertion id="10" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="11" parameters="['hello']" expected="error: 参数有误"/>
        <assertion id="12" parameters="[null]" expected="numeric_001.xml"/>
    </case>

    <case type="query">
        <sql>select castToLong(?);</sql>
        <assertion id="01" parameters="['0']" expected="numeric_002.xml"/>
        <assertion id="02" parameters="['1']" expected="numeric_002.xml"/>
        <assertion id="03" parameters="['-1']" expected="numeric_002.xml"/>
        <assertion id="04" parameters="['12.34']" expected="numeric_002.xml"/>
        <assertion id="05" parameters="['-12.34']" expected="numeric_002.xml"/>
        <assertion id="06" parameters="['9223372036854775807']" expected="numeric_002.xml"/>
        <assertion id="07" parameters="['-9223372036854775808']" expected="numeric_002.xml"/>
        <assertion id="08" parameters="['9223372036854775808']" expected="error: 参数有误"/>
        <assertion id="09" parameters="['-9223372036854775809']" expected="error: 参数有误"/>
        <assertion id="10" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="11" parameters="['hello']" expected="error: 参数有误"/>
        <assertion id="12" parameters="[null]" expected="numeric_002.xml"/>
    </case>

    <case type="query">
        <sql>select castToFloat(?);</sql>
        <assertion id="01" parameters="['0']" expected="numeric_003.xml"/>
        <assertion id="02" parameters="['10000.1234567']" expected="numeric_003.xml"/>
        <assertion id="03" parameters="['-10000.1234567']" expected="numeric_003.xml"/>
        <assertion id="04" parameters="['12345']" expected="numeric_003.xml"/>
        <assertion id="05" parameters="['-12345']" expected="numeric_003.xml"/>
        <assertion id="06" parameters="[10000.1234567]" expected="numeric_003.xml"/>
        <assertion id="07" parameters="[-10000.1234567]" expected="numeric_003.xml"/>
        <assertion id="08" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="09" parameters="['hello']" expected="error: 参数有误"/>
        <assertion id="10" parameters="[null]" expected="numeric_003.xml"/>
    </case>

    <case type="query">
        <sql>select castToDouble(?);</sql>
        <assertion id="01" parameters="['0']" expected="numeric_004.xml"/>
        <assertion id="02" parameters="['100.123456789012345']" expected="numeric_004.xml"/>
        <assertion id="03" parameters="['-100.123456789012345']" expected="numeric_004.xml"/>
        <assertion id="04" parameters="['12345']" expected="numeric_004.xml"/>
        <assertion id="05" parameters="['-12345']" expected="numeric_004.xml"/>
        <assertion id="06" parameters="[100.123456789012345]" expected="numeric_004.xml"/>
        <assertion id="07" parameters="[100.123456789012345]" expected="numeric_004.xml"/>
        <assertion id="08" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="09" parameters="['hello']" expected="error: 参数有误"/>
        <assertion id="10" parameters="[null]" expected="numeric_004.xml"/>
    </case>

    <case type="query">
        <sql>select castToBoolean(?);</sql>
        <assertion id="01" parameters="['true']" expected="numeric_005.xml"/>
        <assertion id="02" parameters="['false']" expected="numeric_005.xml"/>
        <assertion id="03" parameters="['abcd']" expected="error: 参数有误"/>
        <assertion id="04" parameters="['123']" expected="error: 参数有误"/>
        <assertion id="05" parameters="[null]" expected="numeric_005.xml"/>
    </case>

    <case type="query">
        <sql>select castToString(?);</sql>
        <assertion id="01" parameters="[123]" expected="numeric_006.xml"/>
        <assertion id="02" parameters="[-123]" expected="numeric_006.xml"/>
        <assertion id="03" parameters="['abcd']" expected="numeric_006.xml"/>
        <assertion id="04" parameters="[123.456]" expected="numeric_006.xml"/>
        <assertion id="05" parameters="[true]" expected="numeric_006.xml"/>
        <assertion id="06" parameters="[null]" expected="numeric_006.xml"/>
    </case>

    <case type="query">
        <sql>select parseInteger(?);</sql>
        <assertion id="01" parameters="[0]" expected="numeric_007.xml"/>
        <assertion id="02" parameters="[1]" expected="numeric_007.xml"/>
        <assertion id="03" parameters="[-1]" expected="numeric_007.xml"/>
        <assertion id="04" parameters="[10.2]" expected="numeric_007.xml"/>
        <assertion id="05" parameters="[-5.8]" expected="numeric_007.xml"/>
        <assertion id="06" parameters="[2147483647]" expected="numeric_007.xml"/>
        <assertion id="07" parameters="[-2147483648]" expected="numeric_007.xml"/>
        <assertion id="08" parameters="[2147483648.1]" expected="error: 参数有误"/>
        <assertion id="09" parameters="[-2147483649.5]" expected="error: 参数有误"/>
        <assertion id="10" parameters="['abcd']" expected="error: 参数有误"/>
        <assertion id="11" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="12" parameters="[null]" expected="numeric_007.xml"/>
    </case>

    <case type="query">
        <sql>select parseLong(?);</sql>
        <assertion id="01" parameters="[0]" expected="numeric_008.xml"/>
        <assertion id="02" parameters="[123456789]" expected="numeric_008.xml"/>
        <assertion id="03" parameters="[-123456789]" expected="numeric_008.xml"/>
        <assertion id="04" parameters="[12345.2345]" expected="numeric_008.xml"/>
        <assertion id="05" parameters="[-56789.89012]" expected="numeric_008.xml"/>
        <assertion id="06" parameters="[9223372036854775807]" expected="numeric_008.xml"/>
        <assertion id="07" parameters="[-9223372036854775808]" expected="numeric_008.xml"/>
        <assertion id="08" parameters="[9223372036854775807.5]" expected="error: 参数有误"/>
        <assertion id="09" parameters="[-9223372036854775809.5]" expected="error: 参数有误"/>
        <assertion id="10" parameters="['abcd']" expected="error: 参数有误"/>
        <assertion id="11" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="12" parameters="[null]" expected="numeric_008.xml"/>
    </case>

    <case type="query">
        <sql>select  parseDouble(?);</sql>
        <assertion id="01" parameters="[0]" expected="numeric_009.xml"/>
        <assertion id="02" parameters="[12345]" expected="numeric_009.xml"/>
        <assertion id="03" parameters="[-12345]" expected="numeric_009.xml"/>
        <assertion id="04" parameters="[10.23456]" expected="numeric_009.xml"/>
        <assertion id="05" parameters="[-567.89012345]" expected="numeric_009.xml"/>
        <assertion id="06" parameters="['abcd']" expected="error: 参数有误"/>
        <assertion id="07" parameters="[true]" expected="error: 参数有误"/>
        <assertion id="08" parameters="[null]" expected="numeric_009.xml"/>
    </case>

</testCase>